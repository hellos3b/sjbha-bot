{
   "compilerOptions": {
     "strict": true,
     "module": "commonjs",
     "moduleResolution": "node",
     "resolveJsonModule": true,
     "esModuleInterop": true,
     "skipLibCheck": true,
     "forceConsistentCasingInFileNames": true,
     "target": "ESNext",
     "outDir": "build",
     "baseUrl": "src",
     "allowJs": true,
     "checkJs": false,
     "paths": {
       "@sjbha/*": ["./*"]
     }
   }
 }